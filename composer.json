{
    "name": "grena/oxatis-api-php-client",
    "description": "Oxatis client for the API",
    "license": "OSL-3.0",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Adrien PÃ©tremann",
            "email": "hello@grena.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "Heavymind\\Oxatis\\ApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Heavymind\\Oxatis\\ApiClient\\tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "psr/http-message": "^1.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message-implementation": "^1.0",
        "php-http/httplug": "^2.0",
        "php-http/discovery": "^1.6",
        "php-http/multipart-stream-builder": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpunit/phpunit": "^9.5.5",
        "phpspec/phpspec": "^7.1.0",
        "symfony/yaml": "^4.2",
        "donatj/mock-webserver": "^2.0",
        "http-interop/http-factory-guzzle": "^1.0",
        "php-http/guzzle6-adapter": "^2.0"
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.4.20"
        }
    },
    "suggest": {
        "php-http/guzzle6-adapter": "In order to use Guzzle v6 as the HTTP client"
    }
}
